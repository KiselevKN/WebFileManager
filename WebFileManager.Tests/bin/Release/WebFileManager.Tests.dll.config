<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="webFileManagerConfig" type="WebFileManager.Core.Configuration.WebFileManagerConfigurationSection, WebFileManager.Core"/>
  </configSections>
  <webFileManagerConfig>
    <iconSize large="128" medium="64" small="24"/>
    <folderIcon fileBase64="folder" />
    <unknownFileTypeIcon fileBase64="unknownFile" />
    <fileTypeIcons>
      <add fileExtension="txt" fileBase64="txtFile" />
      <add fileExtension="jpg" fileBase64="jpgFile" />
      <add fileExtension="pdf" fileBase64="pdfFile" />
      <add fileExtension="doc" fileBase64="docFile" />
    </fileTypeIcons>
  </webFileManagerConfig>
</configuration>